{"ast":null,"code":"// @ is an alias to /src\nimport request from \"@/utils/request\";\nexport default {\n  name: 'score',\n  components: {},\n\n  data() {\n    return {\n      search: '',\n      currentPage4: 1,\n      pageSize4: 10,\n      total: 0,\n      dialogVisible: false,\n      form: {},\n      tableData: []\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/student\", {\n        params: {\n          pageNum: this.currentPage4,\n          pageSize: this.pageSize4,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      if (this.form.id) {\n        request.put(\"/student\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); //刷新表格的数据\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n      } else {\n        request.post(\"/student\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); //刷新表格的数据\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n      }\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    checkGrade(studentId) {\n      this.$router.push({\n        path: \"/scoreOne\",\n        query: {\n          studentId: studentId\n        }\n      });\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/user/\" + id).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //刷新表格的数剧\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数\n      this.pageSize4 = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //改变当前页码\n      this.currentPage4 = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+DA;AAGA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,MAAI,EAAE,OADO;AAEbC,YAAU,EAAE,EAFC;;AAGbC,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE,EADH;AAELC,kBAAY,EAAE,CAFT;AAGLC,eAAS,EAAE,EAHN;AAILC,WAAK,EAAE,CAJF;AAKLC,mBAAa,EAAC,KALT;AAMLC,UAAI,EAAC,EANA;AAOLC,eAAS,EAAE;AAPN,KAAP;AASD,GAbY;;AAcbC,SAAO,GAAG;AACR,SAAKC,IAAL;AACD,GAhBY;;AAiBbC,SAAO,EAAE;AACPD,QAAI,GAAE;AACJZ,aAAO,CAACc,GAAR,CAAY,UAAZ,EAAwB;AACtBC,cAAM,EAAC;AACLC,iBAAO,EAAC,KAAKX,YADR;AAELY,kBAAQ,EAAC,KAAKX,SAFT;AAGLF,gBAAM,EAAC,KAAKA;AAHP;AADe,OAAxB,EAMGc,IANH,CAMQC,GAAE,IAAK;AACbC,eAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,aAAKT,SAAL,GAAiBS,GAAG,CAAChB,IAAJ,CAASmB,OAA1B;AACA,aAAKf,KAAL,GAAaY,GAAG,CAAChB,IAAJ,CAASI,KAAtB;AACD,OAVD;AAWD,KAbM;;AAcPgB,OAAG,GAAG;AACJ,WAAKf,aAAL,GAAqB,IAArB;AACA,WAAKC,IAAL,GAAY,EAAZ;AAED,KAlBM;;AAmBPe,QAAI,GAAE;AACJ,UAAG,KAAKf,IAAL,CAAUgB,EAAb,EAAgB;AACdzB,eAAO,CAAC0B,GAAR,CAAY,UAAZ,EAAuB,KAAKjB,IAA5B,EAAkCS,IAAlC,CAAuCC,GAAE,IAAK;AAC5CC,iBAAO,CAACC,GAAR,CAAYF,GAAZ;;AACA,cAAGA,GAAG,CAACQ,IAAJ,KAAa,GAAhB,EAAoB;AAClB,iBAAKC,QAAL,CAAc;AACZC,kBAAI,EAAC,SADO;AAEZC,qBAAO,EAAC;AAFI,aAAd;AAID,WALD,MAKK;AACH,iBAAKF,QAAL,CAAc;AACZC,kBAAI,EAAC,OADO;AAEZC,qBAAO,EAA