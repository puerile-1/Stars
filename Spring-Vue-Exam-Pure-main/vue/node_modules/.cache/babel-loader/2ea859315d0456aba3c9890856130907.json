{"ast":null,"code":"// @ is an alias to /src\nimport request from \"@/utils/request\";\nimport { ArrowLeft } from '@element-plus/icons-vue';\nexport default {\n  name: 'adminManage',\n  components: {\n    ArrowLeft\n  },\n\n  data() {\n    return {\n      search: '',\n      currentPage4: 1,\n      pageSize4: 10,\n      total: 0,\n      dialogVisible: false,\n      form: {},\n      tableData: []\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/admin\", {\n        params: {\n          pageNum: this.currentPage4,\n          pageSize: this.pageSize4,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    save() {\n      if (this.form.adminId) {\n        request.put(\"/admin\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); //刷新表格的数据\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n      } else {\n        request.post(\"/admin\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); //刷新表格的数据\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n      }\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/admin/\" + id).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //刷新表格的数剧\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数\n      this.pageSize4 = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //改变当前页码\n      this.currentPage4 = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyFA;AAGA,OAAOA,OAAP,MAAoB,iBAApB;AACA,SAASC,SAAT,QAA0B,yBAA1B;AACA,eAAe;AACbC,MAAI,EAAE,aADO;AAEbC,YAAU,EAAE;AACVF;AADU,GAFC;;AAKbG,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE,EADH;AAELC,kBAAY,EAAE,CAFT;AAGLC,eAAS,EAAE,EAHN;AAILC,WAAK,EAAE,CAJF;AAKLC,mBAAa,EAAC,KALT;AAMLC,UAAI,EAAC,EANA;AAOLC,eAAS,EAAE;AAPN,KAAP;AASD,GAfY;;AAgBbC,SAAO,GAAG;AACR,SAAKC,IAAL;AACD,GAlBY;;AAmBbC,SAAO,EAAE;AACPD,QAAI,GAAE;AACJb,aAAO,CAACe,GAAR,CAAY,QAAZ,EAAsB;AACpBC,cAAM,EAAC;AACLC,iBAAO,EAAC,KAAKX,YADR;AAELY,kBAAQ,EAAC,KAAKX,SAFT;AAGLF,gBAAM,EAAC,KAAKA;AAHP;AADa,OAAtB,EAMGc,IANH,CAMQC,GAAE,IAAK;AACbC,eAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,aAAKT,SAAL,GAAiBS,GAAG,CAAChB,IAAJ,CAASmB,OAA1B;AACA,aAAKf,KAAL,GAAaY,GAAG,CAAChB,IAAJ,CAASI,KAAtB;AACD,OAVD;AAWD,KAbM;;AAcPgB,QAAI,GAAE;AACJ,UAAG,KAAKd,IAAL,CAAUe,OAAb,EAAqB;AACnBzB,eAAO,CAAC0B,GAAR,CAAY,QAAZ,EAAqB,KAAKhB,IAA1B,EAAgCS,IAAhC,CAAqCC,GAAE,IAAK;AAC1CC,iBAAO,CAACC,GAAR,CAAYF,GAAZ;;AACA,cAAGA,GAAG,CAACO,IAAJ,KAAa,GAAhB,EAAoB;AAClB,iBAAKC,QAAL,CAAc;AACZC,kBAAI,EAAC,SADO;AAEZC,qBAAO,EAAC;AAFI,aAAd;AAID,WALD,MAKK;AACH,iBAAKF,QAAL,CAAc;AACZC,kBAAI,EAAC,OADO;AAEZC,qBAAO,EAACV,GAAG,CAACW;AAFA,aAAd;AAIF;;AACA,eAAKlB,IAAL,GAb0C,CAa/B;;AACX,eAAKJ,aAAL,GAAqB,KAArB,CAd0C,CAcd;AAC7B,SAfD;AAgBF,OAjBA,MAkBI;AAC