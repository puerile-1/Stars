{"ast":null,"code":"// @ is an alias to /src\nimport request from \"@/utils/request\";\nexport default {\n  components: {},\n\n  data() {\n    return {\n      search: '',\n      currentPage4: 1,\n      pageSize4: 10,\n      total: 0,\n      dialogVisible: false,\n      form: {},\n      tableData: []\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/score/\" + this.$route.query.studentId, {\n        params: {\n          pageNum: this.currentPage4,\n          pageSize: this.pageSize4\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      if (this.form.id) {\n        request.put(\"/score\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); //刷新表格的数据\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n      } else {\n        request.post(\"/score\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); //刷新表格的数据\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n      }\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    checkGrade() {\n      this.$router.push({\n        path: \"/grade\",\n        query: {\n          studentId: this.$route.query.studentId\n        }\n      });\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/score/\" + id).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n\n        this.load(); //刷新表格的数剧\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数\n      this.pageSize4 = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //改变当前页码\n      this.currentPage4 = pageNum;\n      this.load();\n    },\n\n    goBack() {\n      this.$router.push({\n        path: '/score'\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6DA;AAGA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,YAAU,EAAE,EADC;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE,EADH;AAELC,kBAAY,EAAE,CAFT;AAGLC,eAAS,EAAE,EAHN;AAILC,WAAK,EAAE,CAJF;AAKLC,mBAAa,EAAC,KALT;AAMLC,UAAI,EAAC,EANA;AAOLC,eAAS,EAAE;AAPN,KAAP;AASD,GAZY;;AAabC,SAAO,GAAG;AACR,SAAKC,IAAL;AACD,GAfY;;AAgBbC,SAAO,EAAE;AACPD,QAAI,GAAE;AACJX,aAAO,CAACa,GAAR,CAAY,YAAW,KAAKC,MAAL,CAAYC,KAAZ,CAAkBC,SAAzC,EAAoD;AAClDC,cAAM,EAAC;AACLC,iBAAO,EAAC,KAAKd,YADR;AAELe,kBAAQ,EAAC,KAAKd;AAFT;AAD2C,OAApD,EAKGe,IALH,CAKQC,GAAE,IAAK;AACbC,eAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,aAAKZ,SAAL,GAAiBY,GAAG,CAACnB,IAAJ,CAASsB,OAA1B;AACA,aAAKlB,KAAL,GAAae,GAAG,CAACnB,IAAJ,CAASI,KAAtB;AACD,OATD;AAUD,KAZM;;AAaPmB,OAAG,GAAG;AACJ,WAAKlB,aAAL,GAAqB,IAArB;AACA,WAAKC,IAAL,GAAY,EAAZ;AAED,KAjBM;;AAkBPkB,QAAI,GAAE;AACJ,UAAG,KAAKlB,IAAL,CAAUmB,EAAb,EAAgB;AACd3B,eAAO,CAAC4B,GAAR,CAAY,QAAZ,EAAqB,KAAKpB,IAA1B,EAAgCY,IAAhC,CAAqCC,GAAE,IAAK;AAC1CC,iBAAO,CAACC,GAAR,CAAYF,GAAZ;;AACA,cAAGA,GAAG,CAACQ,IAAJ,KAAa,GAAhB,EAAoB;AAClB,iBAAKC,QAAL,CAAc;AACZC,kBAAI,EAAC,SADO;AAEZC,qBAAO,EAAC;AAFI,aAAd;AAID,WALD,MAKK